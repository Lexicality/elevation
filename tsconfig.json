{
    "compilerOptions": {
        "module": "ES2015",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "sourceMap": false,
        "emitBOM": false,
        "target": "ES2021",
        "removeComments": true,
        "moduleResolution": "node",
        "strict": true
    },
    "compileOnSave": true,
    "include": [
        "src",
        "rollup.config.ts"
    ]
}
